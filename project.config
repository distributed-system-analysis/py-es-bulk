[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = A simple wrapper around elasticsearch-py client streaming_bulk() API with robust error handling
[access "refs/*"]
	owner = group user/Peter Portante (portante)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/Peter Portante (portante)
	label-Verified = -1..+1 group user/Peter Portante (portante)
	push = group user/Peter Portante (portante)
	read = group user/Peter Portante (portante)
	rebase = group user/Peter Portante (portante)
	removeReviewer = group user/Peter Portante (portante)
	submit = group user/Peter Portante (portante)
[access "refs/heads/*"]
	create = group user/Peter Portante (portante)
	push = +force group user/Peter Portante (portante)
	pushMerge = group user/Peter Portante (portante)
	read = group user/Peter Portante (portante)
[access "refs/tags/*"]
	push = group user/Peter Portante (portante)
	push = +force group user/Peter Portante (portante)
